{
  "name": "i18nature-cli",
  "version": "1.0.5",
  "description": "Command-line tool to init, upload and download translation files to I18Nature localization platform.",
  "homepage": "https://github.com/siposdani87/i18nature-cli",
  "bugs": {
    "url": "https://github.com/siposdani87/i18nature-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/siposdani87/i18nature-cli.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "i18nature": "bin/i18nature"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean && npm run format && npm run lint && npm run coverage",
    "build": "npm run webpack",
    "tsc": "npx -p typescript tsc",
    "tsc-test": "npx -p typescript tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "npm run eslint && npm run tsc-test",
    "eslint": "npx eslint --ext ts src/",
    "format": "npx prettier --write 'src/**/*.ts*'",
    "webpack": "npx webpack",
    "eslint:coverage": "npm run eslint -- --format json --output-file coverage/eslint.json",
    "coverage": "npm run eslint:coverage",
    "test": "npx jest"
  },
  "keywords": [
    "i18nature",
    "i18nature-cli",
    "i18n",
    "l10n",
    "translation",
    "localization",
    "internationalization"
  ],
  "author": "DaÃÅniel Sipos <siposdani87@gmail.com> (https://siposdani87.com)",
  "license": "ISC",
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "dependencies": {
    "axios": "^0.27.0",
    "chalk": "^4.1.2",
    "esm": "^3.2.25",
    "fs-extra": "^11.2.0",
    "glob": "^8.0.3",
    "inquirer": "^8.2.0",
    "listr": "^0.14.3",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.0.0",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^28.1.3",
    "@types/listr": "^0.14.4",
    "@types/node": "^14.18.0",
    "@types/yargs": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^28.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.6.0",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
